{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Abysswalker",
  "author": "Andrii Stavskyi",
  "themes": [
    {
      "name": "Abysswalker",
      "appearance": "dark",
      "style": {

        "background": "#1a1a1a",
        "elevated_surface.background": "#1f1f1f",
        "surface.background": "#1a1a1a",
        "panel.background": "#1a1a1a",
        "panel.focused_border": "#bd7e7e",
        "pane.focused_border": "#bd7e7e",

        "text": "#a0a0a0",
        "text.muted": "#666666",
        "text.placeholder": "#444444",
        "text.disabled": "#333333",
        "text.accent": "#bd7e7e",

        "icon": "#a0a0a0",
        "icon.muted": "#666666",
        "icon.accent": "#bd7e7e",

        "border": "#1a1a1a",
        "border.variant": "#252525",
        "border.focused": "#bd7e7e",
        "border.selected": "#bd7e7e",
        "border.transparent": "#00000000",
        "border.disabled": "#333333",

        "editor.background": "#1a1a1a",
        "editor.gutter.background": "#1a1a1a",
        "editor.subheader.background": "#1a1a1a",
        "editor.active_line.background": "#1f1f1f",
        "editor.highlighted_line.background": "#202020",
        "editor.line_number": "#333333",
        "editor.active_line_number": "#f5f5f5",
        "editor.invisible": "#2a2a2a",
        "editor.wrap_guide": "#252525",
        "editor.active_wrap_guide": "#333333",
        "editor.indent_guide": "#252525",
        "editor.indent_guide_active": "#333333",

        "editor.foreground": "#a0a0a0",
        "editor.caret": "#f5f5f5",
        "editor.selection": "#333333",
        "editor.selection.unfocused": "#2a2a2a",

        "tab.active_background": "#1a1a1a",
        "tab.inactive_background": "#1a1a1a",
        "tab_bar.background": "#1a1a1a",

        "toolbar.background": "#1a1a1a",
        "breadcrumb": "#444444",

        "title_bar.background": "#1a1a1a",
        "title_bar.inactive_background": "#1a1a1a",

        "status_bar.background": "#1a1a1a",

        "scrollbar.track.background": "#1a1a1a",
        "scrollbar.track.border": "#1a1a1a",
        "scrollbar.thumb.background": "#252525",
        "scrollbar.thumb.border": "#1a1a1a",
        "scrollbar.thumb.hover_background": "#333333",

        "ghost_element.background": "#00000000",
        "ghost_element.hover": "#202020",
        "ghost_element.active": "#252525",
        "ghost_element.selected": "#252525",
        "ghost_element.disabled": "#1a1a1a",

        "element.background": "#252525",
        "element.hover": "#2a2a2a",
        "element.active": "#333333",
        "element.selected": "#252525",
        "element.disabled": "#1a1a1a",

        "drop_target.background": "#bd7e7e22",

        "button.background": "#bd7e7e",
        "button.foreground": "#1a1a1a",

        "link_text.hover": "#d1c29b",

        "terminal.background": "#1a1a1a",
        "terminal.foreground": "#a0a0a0",
        "terminal.bright_foreground": "#f5f5f5",
        "terminal.dim_foreground": "#666666",
        "terminal.ansi.black": "#1a1a1a",
        "terminal.ansi.red": "#bd7e7e",
        "terminal.ansi.green": "#8fa8bd",
        "terminal.ansi.yellow": "#d1c29b",
        "terminal.ansi.blue": "#8fa8bd",
        "terminal.ansi.magenta": "#bd7e7e",
        "terminal.ansi.cyan": "#8fa8bd",
        "terminal.ansi.white": "#a0a0a0",
        "terminal.ansi.bright_black": "#606060",
        "terminal.ansi.bright_red": "#bd7e7e",
        "terminal.ansi.bright_green": "#8fa8bd",
        "terminal.ansi.bright_yellow": "#d1c29b",
        "terminal.ansi.bright_blue": "#8fa8bd",
        "terminal.ansi.bright_magenta": "#bd7e7e",
        "terminal.ansi.bright_cyan": "#8fa8bd",
        "terminal.ansi.bright_white": "#f5f5f5",

        "syntax": {
          "attribute": { "color": "#a0a0a0" },
          "boolean": { "color": "#bd7e7e" },
          "comment": { "color": "#606060", "font_style": "italic" },
          "comment.doc": { "color": "#606060", "font_style": "italic" },
          "constant": { "color": "#a0a0a0" },
          "constructor": { "color": "#f5f5f5", "font_weight": 700 },
          "embedded": { "color": "#a0a0a0" },
          "emphasis": { "font_style": "italic" },
          "emphasis.strong": { "font_weight": 700 },
          "enum": { "color": "#f5f5f5" },
          "function": { "color": "#8fa8bd" },
          "function.builtin": { "color": "#8fa8bd" },
          "function.call": { "color": "#8fa8bd" },
          "function.definition": { "color": "#8fa8bd" },
          "keyword": { "color": "#bd7e7e" },
          "label": { "color": "#a0a0a0" },
          "link_text": { "color": "#d1c29b" },
          "link_uri": { "color": "#8fa8bd" },
          "number": { "color": "#d1c29b" },
          "operator": { "color": "#757575" },
          "predictive": { "color": "#444444", "font_style": "italic" },
          "preproc": { "color": "#bd7e7e" },
          "primary": { "color": "#f5f5f5" },
          "property": { "color": "#a0a0a0" },
          "punctuation": { "color": "#757575" },
          "punctuation.bracket": { "color": "#757575" },
          "punctuation.delimiter": { "color": "#757575" },
          "punctuation.list_marker": { "color": "#bd7e7e" },
          "punctuation.special": { "color": "#757575" },
          "string": { "color": "#d1c29b" },
          "string.escape": { "color": "#bd7e7e" },
          "string.regex": { "color": "#d1c29b" },
          "string.special": { "color": "#d1c29b" },
          "string.special.symbol": { "color": "#d1c29b" },
          "tag": { "color": "#f5f5f5", "font_weight": 700 },
          "text.literal": { "color": "#d1c29b" },
          "title": { "color": "#f5f5f5", "font_weight": 700 },
          "type": { "color": "#f5f5f5", "font_weight": 700 },
          "type.builtin": { "color": "#bd7e7e" },
          "type.interface": { "color": "#f5f5f5", "font_weight": 700 },
          "type.super": { "color": "#f5f5f5" },
          "variable": { "color": "#a0a0a0" },
          "variable.builtin": { "color": "#bd7e7e" },
          "variable.member": { "color": "#a0a0a0" },
          "variable.parameter": { "color": "#a0a0a0" },
          "variant": { "color": "#f5f5f5" }
        }
      }
    }
  ]
}